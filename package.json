{
  "name": "caLINEdar",
  "version": "1.0.0",
  "author": "<fischer.json@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "webpack": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "node-sass": "^4.8.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "transform-runtime": "^0.0.0",
    "webpack-cli": "^2.0.9"
  },
  "babel": {
    "plugins": ["transform-class-properties", "transform-runtime", "babel-plugin-transform-regenerator"],
    "presets": ["babel-preset-env"]
  },
  "jest": {
    "transform": {
      ".js": "./node_modules/babel-jest"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run copy_assets && cross-env NODE_ENV=production webpack",
    "build-dev": "npm run clean && npm run copy_assets-dev && webpack --watch --info-verbosity verbose",
    "copy_assets": "copyfiles demo/*.html dist/demo -f",
    "copy_assets-dev": "copyfiles demo/*.html devDist/demo -f",
    "clean": "rimraf ./dist && rimraf ./devDist",
    "test": "jest"
  }
}
